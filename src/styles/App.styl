@import "./fonts"
@import "./colors"
html
  box-sizing: border-box;

*, *:before, *:after
  box-sizing: inherit;

a
  text-decoration:none
  color: $blue
.group:after
  content: ""
  display: table
  clear: both

.container
  font-family: "DiaBlack"
  color: $blue
  max-width:1160px
  margin:0 auto
